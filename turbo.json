{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:fast": {
      "dependsOn": ["^build:fast"],
      "outputs": ["dist/**"]
    },
    "ts:emit": {
      "dependsOn": ["^ts:fast"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "coverage": {
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
